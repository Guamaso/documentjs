{
	"versions": {
		"0.1" : "git://github.com/bitovi/documentjs#master"
	},
	"defaultVersion" : "0.1",
	"siteDefaults": {
		"templates": "theme/templates"
	},
	"sites": {
		"pages": {
			"glob": "_pages/*.md",
			"dest": ".",
			"forceBuild": true,
			"pageConfig": {"page":"index"},
			"minifyBuild": false
		}
	}
}
