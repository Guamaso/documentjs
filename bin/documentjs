#!/usr/bin/env node

var yargs = require("yargs");

var document = require("../lib/versions/document");
var only = require("../lib/cmd/only");
var _ = require("underscore");


var options = _.extend({}, yargs.argv);
if(options._.length){
	options.only = only(options._);
}
delete options._;

document({path: process.cwd()}, undefined, options).then(function(){}, function(e){
	setTimeout(function(){
		throw e;
	},1);
});
